#include <bits/stdc++.h>
using namespace std;

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    int n, i;
    string s, s1;
    getline(cin, s1);
    n = s1.size();
    for (i = 0; i < n; i++)
        if (s1[i] == ' ')
            s += "%20";
        else 
            s += s1[i];
    cout << s;
}
